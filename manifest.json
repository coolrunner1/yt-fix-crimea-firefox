{
  "manifest_version": 2,
  "name": "yt-fix",
  "version": "1.0",

  "description": "Fixes video playback for Crimea",

  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["yt-fix.js"]
    }
  ],

  "browser_specific_settings": {
  "gecko": {
    "id": "addon@example.com",
    "strict_min_version": "58.0"
  }
}
}
